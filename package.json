{
  "name": "ts-backend",
  "version": "0.0.1",
  "description": "Backend",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "build:eslint": "tsc -p . && eslint ./dist --ext .js --quiet --fix",
    "start": "node -r dotenv/config ./dist/app.js",
    "dev": "cross-env NODE_ENV=dev node --loader tsx/esm -r dotenv/config src/app.ts",
    "dev:nodemon": "cross-env NODE_ENV=dev nodemon -r dotenv/config ./src/app.ts",
    "eslint:fix": "eslint . --ext .ts --quiet --fix",
    "db:migrate:make": "npx knex migrate:make",
    "db:migrate:up": "npx knex migrate:latest --knexfile knexfile.js",
    "db:migrate:down": "npx knex migrate:rollback --knexfile knexfile.js"
  },
  "keywords": [
    "Backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/swagger": "^8.11.0",
    "@fastify/swagger-ui": "^1.10.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.0",
    "fastify-plugin": "^4.5.1",
    "knex": "^3.0.1",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/node": "^20.8.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "nodemon": "^3.0.1",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  }
}
